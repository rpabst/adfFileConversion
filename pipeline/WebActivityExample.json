{
	"name": "WebActivityExample",
	"properties": {
		"activities": [
			{
				"name": "wbCityData",
				"type": "WebActivity",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"url": " https://jsonplaceholder.typicode.com/posts",
					"method": "POST",
					"headers": {
						"Content-Type": "application/json"
					},
					"body": {
						"value": "@variables('completeJSON')",
						"type": "Expression"
					},
					"datasets": [
						{
							"referenceName": "jsJsonFile",
							"type": "DatasetReference"
						}
					],
					"disableCertValidation": true
				}
			}
		],
		"variables": {
			"surname": {
				"type": "String",
				"defaultValue": "rafael"
			},
			"completeJSON": {
				"type": "String",
				"defaultValue": "{     \"title\": \"BAFFA\",     \"body\": \"jonny\",     \"userId\": 1   }"
			}
		},
		"annotations": []
	}
}